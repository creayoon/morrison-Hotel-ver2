{
  "presets": [
    "es2015",
    "react",
    "stage-0",
    "typescript"
  ],
  "plugins": [
    "transform-object-assign",
    "transform-decorators-legacy"
  ],
  "env": {
    "test": {
      "plugins": [
        [
          "__coverage__",
          {
            "ignore": "src/test/"
          }
        ]
      ]
    }
  }
}
